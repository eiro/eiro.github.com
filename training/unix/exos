mais non non non non non non non
oui

repeat 100 {yes mais non{,,,,,,} |sed 3000q; echo oui } > A 

echo 'yes mais non{,}| sed 2q ; echo oui'

yes 'yes mais non{,} | sed 2q ; echo oui' |
    sed 3q |
    zsh

yes 'yes mais non{,,,} | sed 3000q; echo enfin oui, peut-etre' |
    sed 10q |
    zsh > A
cat A{,,,,,,,,,,} > B


